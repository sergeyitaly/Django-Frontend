 transform 0.3s ease-in-out;\r\n        }\r\n        & .search-btn {\r\n            background: #110B09;\r\n            border-radius: 50%;\r\n            display: flex;\r\n            justify-content: center;\r\n            align-items: center;\r\n            transition: 0.4s;\r\n            color: white;\r\n            text-decoration:none;\r\n        }\r\n    }\r\n\r\n    .input-block {\r\n        flex-direction: column;\r\n        align-items: flex-end;\r\n        width: 27%;\r\n        gap: 30px;\r\n        & .email-input {\r\n            border: solid 3px;\r\n            border-color: #828C76;\r\n            color: white;\r\n            background: none;\r\n            outline: none;\r\n        }\r\n    }\r\n\r\n    input {\r\n        border: 0px;\r\n        padding: 0.5rem;\r\n        outline: none;\r\n        height: 40px;\r\n        border-radius: 5px;\r\n        \r\n        border-color: #828C76;\r\n        background: none;\r\n        color: white;\r\n    }\r\n\r\n    .form-control:focus {\r\n        box-shadow: none;\r\n    }\r\n\r\n    .offcanvas-header {\r\n        & .btn-close {\r\n            background-color: none;\r\n        }\r\n    }\r\n    \r\n    \r\n    \r\n    .container {\r\n        max-width: 1640px;\r\n        padding-left: 15px;\r\n        padding-right: 15px;\r\n    }\r\n\r\n     \r\n\r\n    .carousel {\r\n        border-radius: 30px;\r\n        box-shadow: 5px 5px 10px rgba(0, 0, 0, 0.5);\r\n        & .carousel-inner {\r\n            border-radius: 30px;\r\n        }\r\n    }\r\n\r\n    .content-style {\r\n        display: flex;\r\n        flex-direction: column;\r\n        align-items: center;\r\n        justify-content: center;\r\n        width: 100%;\r\n        height: 100%;\r\n        color: white;\r\n        background: rgba(0,0,0,.4);\r\n        --webkit-filter: blur(0px) saturate(2);\r\n        & .background-shadow{\r\n            display: flex;\r\n            flex-direction: column;\r\n            justify-content: center;\r\n   